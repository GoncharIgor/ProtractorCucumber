{
  "name": "bb-test",
  "version": "1.0.5",
  "description": "A test automation project for Computers Database",
  "repository": {
    "type": "git",
    "url": "https://github.com/GoncharIgor/ProtractorCucumber.git"
  },
  "keywords": [
    "protractor",
    "cucumber",
    "allure reports",
    "angular",
    "angularjs",
    "testing",
    "behaviour driven development",
    "bdd",
    "selenium",
    "webdriverJS",
    "gherkin",
    "automation testing"
  ],
  "dependencies": {
    "chai": "^4.1.2",
    "chai-as-promised": "^7.1.1",
    "cucumber": "^3.0.4",
    "protractor-cucumber-framework": "^4.1.1",
    "lodash": "^4.17.4",
    "minimist": "^0.1.0",
    "mkdirp": "~0.3.5",
    "protractor": "latest",
    "request-promise": "2.0.1",
    "moment": "^2.10.6",
    "otp": "^0.1.2",
    "rimraf": "2.5.1",
    "cucumberjs-allure-reporter": "^1.0.3",
    "cucumber-html-report": "^0.6.0",
    "cucumber-html-reporter": "^3.0.4"
  },
  "scripts": {
    "wd-update": "node ./node_modules/protractor/bin/webdriver-manager update",
    "wd-start": "node ./node_modules/protractor/bin/webdriver-manager start",
    "wd-status": "webdriver-manager status",
    "wd-clean": "webdriver-manager clean",
    "test": "protractor config/comp.js",
    "test-regression": "protractor config/comp.js --cucumberOpts.tags @regression --disableChecks",
    "test-amount": "protractor config/comp.js --cucumberOpts.tags @amount --disableChecks"
  }
}
